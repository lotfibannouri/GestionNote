@model Etudiant
<div>
    <h3>Créer un nouveau Etudiant</h3>

</div>
<form class="needs-validation" novalidate="" asp-controller="Etudiant" asp-action="AddEtudiant" method="post">
          <div class="row g-3">
            <div class="col-sm-6">
              <label for="firstName" class="form-label">Nom</label>
              <input type="text" class="form-control" id="nom" placeholder="" value="" required="" asp-for="Nom">
             
            </div>

            <div class="col-sm-6">
              <label for="lastName" class="form-label">Prénom</label>
              <input type="text" class="form-control" id="prénom" placeholder="" value="" required="" asp-for="Prenom">
             
            </div>
         
          </div>

          <hr class="my-4">


          <button class="w-100 btn btn-primary btn-lg" type="submit">Enregistrer</button>
        </form>